  </CharacterStyleRange>
  <CharacterStyleRange AppliedCharacterStyle="CharacterStyle/Italic">
    <Content>Hybrid Workflow How-To: Making Automated Workflows, Part 1</Content>
  </CharacterStyleRange>
  <CharacterStyleRange AppliedCharacterStyle="$ID/NormalCharacterStyle">
    <Content>) </Content>
  </CharacterStyleRange>
  <HyperlinkTextSource Self="htss-3" Name="Link to blog post: Hybrid workflow how-to: Making automated workflows, part 2" Hidden="false">
    <CharacterStyleRange AppliedCharacterStyle="CharacterStyle/Link">
